
PROGRAMS = dbu
targets  = $(PROGRAMS)

PROGRAM_LDADD        = $(EXTRA_LDFLAGS) $(PROGRAM_DEPENDENCIES) $(EXTRA_LIBS)
PROGRAM_DEPENDENCIES = ../ap/libap.la \
	../lib/aputil/libaputil.la \
	../lib/sdbm/libsdbm.la \
	../lib/apr/$(LIBPRE)apr.a

include $(top_srcdir)/build/rules.mk

dbu_OBJECTS = dbu.lo
dbu: $(dbu_OBJECTS)
	$(LINK) $(dbu_OBJECTS) $(PROGRAM_LDADD)
